{
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "complexity": [2, 16],
        "max-nested-callbacks": [2, 3],
        "max-params": [2, 3],
        "max-statements": [2, 30],
        "no-extra-parens": [0],
        "no-magic-numbers": [0],
        "no-restricted-syntax": [2, "ContinueStatement", "DebuggerStatement", "LabeledStatement", "WithStatement"],
        "operator-linebreak": [2, "before"],
        "spaced-comment": [0],
        "strict": [0]
    }
}